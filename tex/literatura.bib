@TECHREPORT{X690,
  title = {{ITU-T} {R}ecommendation {X}.690: {ASN}.1 encoding rules: {S}pecification
	of {B}asic {E}ncoding {R}ules ({BER}), {C}anonical {E}ncoding {R}ules
	({CER}) and {D}istinguished {E}ncoding {R}ules ({DER})},
  institution = {International {T}elecommunication {U}nion},
  year = {2002},
  type = {Standarization document},
  month = {July},
  abstract = {This {R}ecommendation | {I}nternational {S}tandard defines a set of
	{B}asic {E}ncoding {R}ules ({BER}) that may be applied to values
	of types defined using the ASN.1 notation. Application of these encoding
	rules produces a transfer syntax for such values. It is implicit
	in the specification of these encoding rules that they are also used
	for decoding. This Recommendation | International Standard defines
	also a set of Distinguished Encoding Rules (DER) and a set of Canonical
	Encoding Rules (CER) both of which provide constraints on the Basic
	Encoding Rules (BER). The key difference between them is that DER
	uses the definite length form of encoding while CER uses the indefinite
	length form. DER is more suitable for the small encoded values, while
	CER is more suitable for the large ones. It is implicit in the specification
	of these encoding rules that they are also used for decoding.},
  keywords = {model, network, standard},
  school = {{ITU-T} {S}tudy {G}roup 17},
  series = {{ITU-T} {S}eries {X}: {D}ata {N}etworks and {O}pen {S}ystem {C}ommunications},
  url = {http://www.itu.int/ITU-T/studygroups/com17/languages/X.690-0207.pdf}
}

